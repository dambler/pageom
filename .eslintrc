{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint",
    "jest",
    "import",
    "prettier"
  ],
  "rules": {
    "prettier/prettier": ["error"],
    "import/extensions": "off"
  },
  "extends": [
    "airbnb-base", 
    "prettier", 
    "plugin:import/recommended", 
    "plugin:import/typescript"
  ],
  "env": {
    "jest/globals": true
  }
}